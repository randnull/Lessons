    </insert>
    <insert tableName="tags">
        <column name="id" valueComputed="gen_random_uuid()"/>
        <column name="tag_name" value="физика"/>
    </insert>
    <insert tableName="tags">
        <column name="id" valueComputed="gen_random_uuid()"/>
        <column name="tag_name" value="информатика"/>
    </insert>
    <insert tableName="tags">
        <column name="id" valueComputed="gen_random_uuid()"/>
        <column name="tag_name" value="английский"/>
    </insert>
    <insert tableName="tags">
        <column name="id" valueComputed="gen_random_uuid()"/>
